    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PId">
            <Value>=Parameters!PId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @from datetime,@to datetime,@PId Int
--set @from ='2022-08-18' set @to ='2022-08-18' set @PId=0

select  distinct AreaId,isnull(AreaName,'No Name'),PrabhagName,TotalCollected,Liquid_Count
from(

select    lm.areaid AreaId,
         (select area from teritorymaster where Id=lm.AreaId ) AreaName, 
		 (select distinct(cm.CommitteeName) from CommitteeMaster cm Where cm.Id=gc.PrabhagId) PrabhagName,
		 count(gc.gcId)  TotalCollected,
		 (select  count(g.gcId) from GarbageCollectionDetails g,LiquidWasteDetails l 
		  where g.LWId=l.LWId 	
		 and cast(g.gcDate as date) between @from and @to		  
		 and isnull(lm.AreaId,0)=isnull(l.AreaId,0)		  
		 ) Liquid_Count

		 	 
from GarbageCollectionDetails gc,
     LiquidWasteDetails lm	 
where gc.LWId=lm.LWId
and   gc.gctype=4
and cast(gc.gcDate as date) between @from and @to
And (gc.PrabhagId=@PId OR @PId Is Null OR @PId=0)
group by lm.AreaId,gc.PrabhagId
) x</CommandText>
      </Query>
      <Fields>
        <Field Name="AreaId">
          <DataField>AreaId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PrabhagName">
          <DataField>PrabhagName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalCollected">
          <DataField>TotalCollected</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Liquid_Count">
          <DataField>Liquid_Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
