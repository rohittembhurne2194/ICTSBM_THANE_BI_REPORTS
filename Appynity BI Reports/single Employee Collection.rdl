    <DataSet Name="Collection_Detail">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@userid">
            <Value>=Parameters!userid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PId">
            <Value>=Parameters!PId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @from datetime,@to datetime,@userid int,@PId int
--set @from ='2022-08-12' set @to ='2022-08-12' set @userid =7 set @PId=null

select 
 distinct cast(gc.gcDate as date) Date,
 gc.userid,
 um.username Name,
 count(gc.gcId) Total_Collection
from GarbageCollectionDetails gc,
     usermaster um
where cast(gc.gcDate as date) between @from and @to
and   um.userId=gc.userId
and  ( um.EmployeeType Is Null OR um.EmployeeType='CT')
and   gc.userId=@userid
and   (gc.gctype=1 OR gc.gcType=6 OR gc.gcType=7 OR gc.gcType=9 OR gc.gcType=10 OR gc.gcType=12 OR gc.gcType=13)
and (gc.PrabhagId= @PId OR @PId IS NULL OR @PId=0)
group by cast(gc.gcDate as date),gc.userId,um.userName
order by date</CommandText>
      </Query>
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="userid">
          <DataField>userid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Collection">
          <DataField>Total_Collection</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
